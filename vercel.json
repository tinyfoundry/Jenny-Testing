{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate" },
        { "key": "Pragma", "value": "no-cache" },
        { "key": "Expires", "value": "0" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/about", "destination": "/" },
    { "source": "/events", "destination": "/" },
    { "source": "/jacksonville", "destination": "/" },
    { "source": "/submit", "destination": "/" }
  ]
}
